<Controls:MetroWindow x:Class="ERSApp.Views.StaffSessionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        GlowBrush="{DynamicResource AccentColorBrush}"
        WindowStartupLocation="CenterOwner"
        Title="Staff View" Height="750" Width="760">
    <Grid Name="grdStaff" Margin="15,0,15,15">
        <Label Name ="lblHeader" Content="Day - Date" FontSize="26" HorizontalAlignment="Left" Foreground="{DynamicResource AccentColorBrush}"/>
        <Label Content="Clinic Time:" HorizontalAlignment="Left" Margin="0,50,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox Name="txtClinicTime" HorizontalAlignment="Left" Height="23" Margin="85,50,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="150" IsEnabled="False"/>
        <Label Content="Chairs:" HorizontalAlignment="Left" Margin="25,90,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox Name="txtChairs" HorizontalAlignment="Left" Height="23" Margin="85,90,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="150" IsEnabled="False"/>
        <Label Content="LOD:" HorizontalAlignment="Left" Margin="35,130,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox Name="txtLOD" HorizontalAlignment="Left" Height="23" Margin="85,130,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="150" IsUndoEnabled="False" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}"/>
        <Label Content="Type:" HorizontalAlignment="Right" Margin="0,50,245,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox Name="txtType" HorizontalAlignment="Right" Height="23" Margin="0,50,85,0" TextAlignment="Right" VerticalAlignment="Top" Width="150" IsEnabled="False"/>
        <Label Content="OCC:" HorizontalAlignment="Right" Margin="0,90,245,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox Name="txtOCC" HorizontalAlignment="Right" Height="23" Margin="0,90,85,0" TextAlignment="Right" VerticalAlignment="Top" Width="150" IsEnabled="False"/>
        <Label Content="Estimate:" HorizontalAlignment="Right" Margin="0,130,245,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox Name="txtEstimate" HorizontalAlignment="Right" Height="23" Margin="0,130,85,0" TextAlignment="Right" VerticalAlignment="Top" Width="150" IsEnabled="False"/>

        <Separator HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,199,0,0" Width="330" Background="{DynamicResource AccentColorBrush3}"></Separator>
        <Separator HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,199,0,0" Width="330" Background="{DynamicResource AccentColorBrush3}"></Separator>
        <Label Content="Staff" HorizontalAlignment="Center" Margin="0,180,0,0" VerticalAlignment="Top" FontWeight="DemiBold" FontSize="18" Foreground="{DynamicResource AccentColorBrush}"/>
        <Label Content="Supervisor:" HorizontalAlignment="Center" Margin="0,230,340,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <ComboBox Name="cboSV1" HorizontalAlignment="Center" Height="23" Margin="0,230,100,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=SVList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboSV1_SelectionChanged"/>
        <TextBox Name="txtSV1LOD" HorizontalAlignment="Center" Height="23" Margin="120,230,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtSV1UNS" HorizontalAlignment="Center" Height="23" Margin="240,230,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetSV1" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  HorizontalAlignment="Center" Margin="330,230,0,0" VerticalAlignment="Top" Width="23" Height="23" IsEnabled="False" Click="btnResetSV1_Click" />
        <Label Content="Drivers: " HorizontalAlignment="Center" Margin="0,270,320,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <ComboBox Name="cboDRI1" HorizontalAlignment="Center" Height="23" Margin="0,270,100,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=DRIList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboDRI1_SelectionChanged"/>
        <TextBox Name="txtDRI1LOD" HorizontalAlignment="Center" Height="23" Margin="120,270,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtDRI1UNS" HorizontalAlignment="Center" Height="23" Margin="240,270,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetDRI1" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  HorizontalAlignment="Center" Margin="330,270,0,0" VerticalAlignment="Top" Width="23" Height="23" IsEnabled="False" Click="btnResetDRI1_Click"/>
        <ComboBox Name="cboDRI2" HorizontalAlignment="Center" Height="23" Margin="0,310,100,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=DRIList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False" SelectionChanged="cboDRI2_SelectionChanged"/>
        <TextBox Name="txtDRI2LOD" HorizontalAlignment="Center" Height="23" Margin="120,310,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtDRI2UNS" HorizontalAlignment="Center" Height="23" Margin="240,310,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetDRI2" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  HorizontalAlignment="Center" Margin="330,310,0,0" VerticalAlignment="Top" Width="23" Height="23"  IsEnabled="False" Click="btnResetDRI2_Click"/>
        <Label Content="RNs: " HorizontalAlignment="Left" Margin="0,360,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <ComboBox Name="cboRN1" HorizontalAlignment="Left" Height="23" Margin="50,360,0,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=RNList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboRN1_SelectionChanged"/>
        <TextBox Name="txtRN1LOD" HorizontalAlignment="Left" Height="23" Margin="210,360,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtRN1UNS" HorizontalAlignment="Left" Height="23" Margin="270,360,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetRN1" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  HorizontalAlignment="Left" Margin="330,360,0,0" VerticalAlignment="Top" Width="23" Height="23" IsEnabled="False" Click="btnResetRN1_Click"/>
        <ComboBox Name="cboRN2" HorizontalAlignment="Left" Height="23" Margin="50,400,0,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=RNList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboRN2_SelectionChanged" IsEnabled="False"/>
        <TextBox Name="txtRN2LOD" HorizontalAlignment="Left" Height="23" Margin="210,400,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtRN2UNS" HorizontalAlignment="Left" Height="23" Margin="270,400,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetRN2" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  HorizontalAlignment="Left" Margin="330,400,0,0" VerticalAlignment="Top" Width="23" Height="23" IsEnabled="False" Click="btnResetRN2_Click"/>
        <ComboBox Name="cboRN3" HorizontalAlignment="Left" Height="23" Margin="50,440,0,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=RNList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboRN3_SelectionChanged" IsEnabled="False"/>
        <TextBox Name="txtRN3LOD" HorizontalAlignment="Left" Height="23" Margin="210,440,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtRN3UNS" HorizontalAlignment="Left" Height="23" Margin="270,440,0,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetRN3" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  IsEnabled="False" HorizontalAlignment="Left" Margin="330,440,0,0" VerticalAlignment="Top" Width="23" Height="23" Click="btnResetRN3_Click"/>
        <Label Content="CCAs: " HorizontalAlignment="Right" Margin="0,360,313,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <ComboBox Name="cboCCA1" HorizontalAlignment="Right" Height="23" Margin="0,360,153,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=CCAList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboCCA1_SelectionChanged"/>
        <TextBox Name="txtCCA1LOD" HorizontalAlignment="Right" Height="23" Margin="0,360,93,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtCCA1UNS" HorizontalAlignment="Right" Height="23" Margin="0,360,33,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetCCA1" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  HorizontalAlignment="Right" Margin="0,360,0,0" VerticalAlignment="Top" Width="23" Height="23" IsEnabled="False" Click="btnResetCCA1_Click" />
        <ComboBox Name="cboCCA2" HorizontalAlignment="Right" Height="23" Margin="0,400,153,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=CCAList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboCCA2_SelectionChanged" IsEnabled="False"/>
        <TextBox Name="txtCCA2LOD" HorizontalAlignment="Right" Height="23" Margin="0,400,93,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtCCA2UNS" HorizontalAlignment="Right" Height="23" Margin="0,400,33,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetCCA2" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4"  IsEnabled="False" HorizontalAlignment="Right"  Margin="0,400,0,0" VerticalAlignment="Top" Width="23" Height="23" Click="btnResetCCA2_Click"/>
        <ComboBox Name="cboCCA3" HorizontalAlignment="Right" Height="23" Margin="0,440,153,0" VerticalAlignment="Top" Width="150" ItemsSource="{Binding Path=CCAList}" DisplayMemberPath="Name" SelectedValuePath="Id" BorderBrush="{DynamicResource AccentColorBrush}" SelectionChanged="cboCCA3_SelectionChanged" IsEnabled="False"/>
        <TextBox Name="txtCCA3LOD" HorizontalAlignment="Right" Height="23" Margin="0,440,93,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <TextBox Name="txtCCA3UNS" HorizontalAlignment="Right" Height="23" Margin="0,440,33,0" TextAlignment="Right" VerticalAlignment="Top" Width="50" PreviewTextInput="NumberValidationTextBox" MaxLength="4" BorderBrush="{DynamicResource AccentColorBrush}" IsEnabled="False"/>
        <Button Name="btnResetCCA3" Content="{DynamicResource DeleteIcon}" BorderBrush="Transparent" Background="Transparent" Padding="4" IsEnabled="False" HorizontalAlignment="Right" Margin="0,440,0,0" VerticalAlignment="Top" Width="23"  Height="23" Click="btnResetCCA3_Click"/>
        <lvc:CartesianChart Margin="0,480,0,40" LegendLocation="Bottom" DataTooltip="{x:Null}" Hoverable="False" Series="{Binding SeriesCollection}">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Name="xAxis" MinValue="0" MaxValue="1">
                    <lvc:Axis.Separator>
                        <lvc:Separator Step="1" />
                    </lvc:Axis.Separator>
                </lvc:Axis>
            </lvc:CartesianChart.AxisX>
            <lvc:CartesianChart.AxisY>
                <lvc:Axis ShowLabels="False" />
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>
        <ContentControl Content="{DynamicResource WBSIcon}" VerticalAlignment="Bottom" Height="27" Margin="0,0,0,-27"/>
        <Button Content="Update" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Bottom" Width="75" IsDefault="True" Click="btnUpdate_Click"/>
        <Button Content="Auto LOD" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Bottom" Width="75" IsDefault="True" Click="btnAutoLOD_Click"/>
        <Button Content="Team" HorizontalAlignment="Right" Margin="0,0,90,0" VerticalAlignment="Bottom" Width="75" Click="btnTeam_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Right" Margin="0,0,0,0" VerticalAlignment="Bottom" Width="75" IsCancel="True" Click="btnClose_Click"/>
    </Grid>
</Controls:MetroWindow>
